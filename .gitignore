# Ignore environment variables
.env
.env.*
!.env.example

# Ignore Rails specific files
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep
/storage/*
!/storage/.keep
/tmp/storage/*
!/tmp/storage/.keep
/tmp/cache/*
!/tmp/cache/.keep
/tmp/cache/assets/*
!/tmp/cache/assets/.keep

# Ignore Node modules
/node_modules
/yarn-error.log
yarn-debug.log*
.yarn-integrity

# Ignore database
*.sqlite3
*.sqlite3-*
*.sqlite3-journal

# Ignore test coverage
/coverage

# Ignore system files
.DS_Store
Thumbs.db 